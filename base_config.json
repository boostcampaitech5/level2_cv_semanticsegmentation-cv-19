{
    "name": "Baseline",
    "seed": 21,
    "epochs": 250,
    "early_stop": 10,
    "save_interval": 1,
    "log_interval": 20,
    "is_wandb": true,
    "is_debug": false,
    "dataset": "XRayDataset",
    "augmentation": "CustomAugmentation",
    "batch_size": 4,
    "model": "UNet",
    "criterion": [
        "cross_entropy",
        "focal",
        "bce_with_logit",
        "dice",
        "iou",
        "combo_loss",
        "tversky",
        "focal_tversky",
        "lovaz"
    ],
    "optimizer": {
        "type": "AdamW",
        "args": {
            "lr": 5e-4,
            "weight_decay": 5e-5
        }
    },
    "lr_scheduler": {
        "type": "ReduceLROnPlateau",
        "args": {
            "mode": "min",
            "factor": 0.1,
            "patience": 3,
            "verbose": true
        }
    },
    "num_workers": 8
}