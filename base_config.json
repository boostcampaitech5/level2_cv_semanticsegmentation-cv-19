{
    "name": "Baseline",
    "root_dir": "/opt/ml/data",
    "seed": 21,
    "epochs": 250,
    "early_stop": 10,
    "save_interval": 1,
    "log_interval": 20,
    "is_wandb": true,
    "is_debug": false,
    "smp": {
        "use": false,
        "args": {
            "encoder_name": "efficientnet-b7",
            "encoder_weights": "imagenet",
            "in_channels": 3,
            "classes": 29
        }
    },
    "dataset": "XRayDataset",
    "augmentation": "CustomAugmentation",
    "batch_size": 8,
    "iters_to_accumulate": 4,
    "model": "UNet",
    "criterion": [
        "focal",
        "bce_with_logit",
        "dice",
        "iou",
        "combo_loss",
        "tversky",
        "focal_tversky",
        "lovaz"
    ],
    "optimizer": {
        "type": "AdamW",
        "args": {
            "lr": 1e-4,
            "weight_decay": 1e-5
        }
    },
    "lr_scheduler": {
        "type": "ReduceLROnPlateau",
        "args": {
            "mode": "min",
            "factor": 0.1,
            "patience": 3,
            "verbose": true
        }
    },
    "num_workers": 4
}